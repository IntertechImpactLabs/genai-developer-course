{
  "name": "genai-developer-course",
  "version": "1.0.0",
  "description": "GenAI Developer Course workspace",
  "private": true,
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format": "black .",
    "build": "tsc -b",
    "build:currency": "tsc -p section1/03_currency_formatter_exercise",
    "build:cache": "tsc -p section2/02_cache_helper_exercise",
    "clean": "rm -rf section*/*/dist",
    "dev:currency": "ts-node section1/03_currency_formatter_exercise/src/currencyFormatter.ts",
    "dev:cache": "ts-node section2/02_cache_helper_exercise/src/cache.ts",
    "start:refactoring": "node section3/01_refactoring_demo/src/server.js",
    "dev:refactoring": "nodemon section3/01_refactoring_demo/src/server.js",
    "init-db": "node section3/01_refactoring_demo/src/init-db.js"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.2.1",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  }
}
